sap.ui.define([                                                                                                                                                                                                                                                
    "sap/m/MessageToast"                                                                                                                                                                                                                                       
], function(MessageToast) {                                                                                                                                                                                                                                    
    'use strict';                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
    return {                                                                                                                                                                                                                                                   
        show_output: function(oEvent) {                                                                                                                                                                                                                        
            MessageToast.show("Custom handler invoked.");                                                                                                                                                                                                      
        }                                                                                                                                                                                                                                                      
    }                                                                                                                                                                                                                                                          
});                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               